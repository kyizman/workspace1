public class TriangleFromSimpleGeometricObject {
    private Point A;
    private Point B;
    private Point C;

    
    

    
    private double getarea(){
        double s = (A + B + C)/2;

        double area = Math.sqrt(s*(s-A)*(s-B)*(s-C));
        return area;

    }
