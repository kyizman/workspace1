import java.util.Scanner;

public class Lab_2_5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner input = new Scanner(System.in);
	System.out.println("Input numbers of pennys,Nickles,Dimes,Quarters");
System.out.println("Pennys:" );
	double Pennies = input.nextDouble(); Pennies = 1;
	System.out.println("Nickles");
	double Nickles = input.nextDouble(); Nickles = 5;
	System.out.println("Dimes:");
	double Dimes = input.nextDouble(); Dimes = 10;
	System.out.println("Quaters:");
	double Quarters = input.nextDouble();
		System.out.println("Total =" + (Pennies + Nickles + Dimes + Quarters)  );
	
	}
	


}
