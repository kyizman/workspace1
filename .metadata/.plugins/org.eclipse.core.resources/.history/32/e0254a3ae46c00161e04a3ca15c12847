import java.util.Scanner;
public class SumDigits {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner(System.in);
	System.out.println("Enter a number that is less than 1000 (positive)");
	int x = input.nextInt();
	
	 int a = x/100;
	 int b = x % 100;
	 int c = b / 10;
	 int d = c % 10;
	 int e = c/1
			 int f = c % 1 ;
 
 System.out.println(" the sum of the diggits is" + (a + c + d + e));
	}
	}
	
