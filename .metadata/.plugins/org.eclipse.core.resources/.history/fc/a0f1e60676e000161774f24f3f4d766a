import java.util.*;
public class Assinement1 {

	public static void main(String[] args) {
	
		double[] list = {4758.806 ,5795.711,5014.683 ,7701.312,	3246.816,6989.659,5027.265,7440.575,3019.995,7241.105
				,7696.65,7098.486,4367.961,5519.468,2894.973,7202.648,6443.892,3033.452,3223.899,6697.127
				,4898.63,	2467.281,2487.543,5172.012,3455.382,7849.135,3166.272,4926.252,7524.989,7755.748
				,6438.85,5302.584,5458.334,5804.818,3185.936,5759.344,7182.593,7735.632,6649.255,6619.317
				,5909.641,6634.304,7318.31, 2923.583,5803.122,6958.059,5218.86,	6094.804,3096.456,4922.01,7275.775,3607.358};

	
	System.out.print(Arrays.toString(list));
	System.out.println();
	
	double[] listrev = reverse(list);
	
System.out.println(Arrays.toString(listrev));
	}

public static  double[] reverse (double[] x){

	int up =0;
	double[] rev = new double[x.length];
	for(int z=90;z>=0; z--)
	{
		rev[z]=x[up];
		up++;
	}
	

return rev;	
};


}
